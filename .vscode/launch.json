{
  "version": "0.2.0",
  "configurations": [{
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/_mocha",
      "sourceMaps": true,
      "args": [
          "-r",
          "tsconfig-paths/register",
          "-r",
          "ts-node/register",
          "${relativeFile}",
          "--colors",
          "-t 100000",
          "node"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
  }]
}